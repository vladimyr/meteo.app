{
  "name": "meteo.app",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build:js": "browserify -t bubleify index.js > dist/bundle.js",
    "build:css": "stylus -c style.styl -o dist/style.css",
    "build": "npm run build:js && npm run build:css",
    "watch:js": "watchify index.js -v -t bubleify -d -o dist/bundle.js",
    "watch:css": "stylus -w style.styl -o dist/style.css",
    "watch": "npm run watch:js | npm run watch:css",
    "deploy": "surge -d https://crometeo.surge.sh -p ./dist"
  },
  "author": "vladimyr",
  "license": "MIT",
  "devDependencies": {
    "browserify": "^13.3.0",
    "bubleify": "^0.7.0",
    "cash-dom": "^1.3.5",
    "stylus": "^0.54.5",
    "watchify": "^3.8.0"
  },
  "dependencies": {
    "qwest": "^4.4.5",
    "redom": "^2.4.1",
    "url-join": "^1.1.0"
  }
}
